{
	"compilerOptions": {
		"target": "es2018",
		"module": "commonjs",
		"lib": ["es2018", "esnext.asynciterable", "DOM"],
		"resolveJsonModule": false,
		"declaration": true,
		"outDir": "./dist",
		"rootDir": "./src",
		"strict": true,
		"strictPropertyInitialization": false,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	}
}
